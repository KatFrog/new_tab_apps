{
  "manifest_version": 2,

  "name": "new_tab_apps",
  "description":  "Chrome extension to replace new tab page with installed apps";
  "default_locale": "en",
  "version": "1.0",
  "chrome_url_overrides": {
     "newtab": "apps.html"
  }
  "permissions": ["tabs", "storage"],
  "incognito": "split",
  "minimum_chrome_version": "33"
} 
